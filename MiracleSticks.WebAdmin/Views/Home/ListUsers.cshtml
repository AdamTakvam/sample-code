@model IList<MiracleSticks.Model.UserAccount>

@{
    ViewBag.Title = "Users";
}

<h2>Users:</h2>

<table>
<tr><td>Last Name</td><td>First Name</td><td>UserName</td><td>Group ID</td><td>Actions</td></tr>

@foreach (var user in Model)
{
    <tr>
        <td>@user.LastName</td>
        <td>@user.FirstName</td> 
        <td>@user.UserName</td>
        <td>@user.GroupID</td>         
        <td>@Html.ActionLink("List Registrations", "ListRegistrations", new { userName = user.UserName } ) | @Html.ActionLink("Delete", "DeleteUser", new { userName = user.UserName } )</td>
    </tr>
}

</table>
